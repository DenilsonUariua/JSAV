<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
  {% for page in site.pages %}
   {% comment %}Couldn't figure out how to check if string not contains something, so stupid empty if block{% endcomment %}
    {% if page.url contains '404' or page.url contains 'sitemap.xml' %}{% else %}
    <url>
      <loc>http://jsav.io{{ page.url }}</loc>
      <changefreq>weekly</changefreq>
      <priority>0.80</priority>
    </url>
  {% endif %}
  {% endfor %}
</urlset>